      machine_name=((String)Report.GetColumnValue(&quot;Reciept.MachineNo&quot;));
      sumtotal=((String)Report.GetColumnValue(&quot;Reciept.TotalAmount&quot;));
    }

    private void Data1_AfterLayout(object sender, EventArgs e)
